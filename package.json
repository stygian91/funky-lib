{
  "name": "funky-lib",
  "version": "7.6.2",
  "description": "",
  "main": "dist/index.js",
  "module": "es/index.js",
  "unpkg": "umd/main.js",
  "files": [
    "es",
    "dist",
    "umd",
    "src"
  ],
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf es/* dist/* umd/*",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist",
    "build:umd": "webpack --mode=production",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prettier": "prettier --write ./src"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.2.4",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "cross-env": "^7.0.2",
    "jest": "^27",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "jshintConfig": {
    "esversion": "9",
    "maxcomplexity": "7"
  },
  "repository": "github:stygian91/funky-lib",
  "sideEffects": false
}
